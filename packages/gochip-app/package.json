{
  "name": "gochip",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ra:d": "react-native run-android --variant=devDebug --appIdSuffix debug",
    "ra:b": "react-native run-android --variant=betaRelease --appIdSuffix beta --no-packager",
    "ra:p": "react-native run-android --variant=prodRelease --no-packager",
    "gradle:b": "cd ./android && ./gradlew assembleBetaRelease && cd ..",
    "gradle:p": "cd ./android && ./gradlew assembleProdRelease && cd ..",
    "ios": "react-native run-ios --simulator \"iPhone 7\" ",
    "ios:x": "react-native run-ios --simulator \"iPhone X\" ",
    "bundles:ios": "npm run getSvg && rm -rf bundles/ios/** && react-native bundle --reset-cache --platform ios --dev false --entry-file ./index.js  --bundle-output bundles/ios/main.jsbundle --assets-dest bundles/ios/",
    "bundles:android": "npm run getSvg && rm -rf bundles/android/** && react-native bundle --reset-cache --platform android --dev false --entry-file ./index.js  --bundle-output bundles/android/index.android.bundle --assets-dest bundles/android/",
    "bundles": "npm run getSvg && npm run bundles:ios && npm run bundles:android",
    "clearCache:": "watchman watch-del-all",
    "link": "react-native link",
    "podInstall": "cd ios && pod install && cd ..",
    "autoInstall": "yarn install && jetify && npm run podInstall && npm run getSvg",
    "getSvg": "node ./src/svgs/getSvg.js",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^1.0.2",
    "@ant-design/react-native": "^3.1.8",
    "@react-native-community/async-storage": "^1.5.1",
    "autoresponsive-core": "^1.0.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "native-base": "^2.12.2",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.3",
    "react-native-barcode-mask": "^1.0.5",
    "react-native-camera": "^3.0.0",
    "react-native-code-push": "^5.6.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-i18n": "^2.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-modal": "^11.1.0",
    "react-native-nfc-manager": "^2.0.0-beta.1",
    "react-native-picker-select": "^6.3.0",
    "react-native-reanimated": "^1.1.0",
    "react-native-segmented-control-tab": "^3.4.1",
    "react-native-shadow": "^1.2.2",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-svg": "^9.4.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-swiper": "^1.5.14",
    "react-native-syan-image-picker": "^0.3.7",
    "react-native-waterfall": "^0.0.8",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
